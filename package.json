  "description": "react material icon picker component",
  "main": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --config webpack.prod.js",
    "jest": "jest",
    "cypress:chrome": "cypress run --component --browser chrome",
    "cypress:firefox": "cypress run --component --browser firefox",
    "cypress:edge": "cypress run --component --browser edge",
    "cypress:electron": "cypress run --component --browser electron",
    "cypress:all": "npm run cypress:chrome && npm run cypress:firefox && npm run cypress:edge && npm run cypress:electron",
    "cypress:open": "cypress open",
    "prettier": "npx prettier . --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KurtXiaoZ/react-material-icon-picker.git"
  },
